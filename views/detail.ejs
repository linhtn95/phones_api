<!DOCTYPE html>
<html lang="en">

<head>
    <% include partials/head %>
</head>

<body>
    <!-- Navigation -->
    <% include partials/header %>

    <!-- Page Content -->
    <div class="container">
        <div class="row" style="margin-top: 50px">
            <div class="col-md-8">
                <% phone.img.forEach(function(el){ %>
                    <img src="http://localhost:3000/<%= el%>" style="width:300px; height: 300px;">
                <% }); %>
            </div>
            <div class="col-md-4">
                <h3><%= phone.brand%> <%= phone.name%></h3>
                <h6>Price<span class="badge badge-danger">$<%= phone.price%></span></h6>
            </div>
        </div>
    </div>
    <% include partials/footer %>
    <!-- Bootstrap core JavaScript -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

</body>

</html>